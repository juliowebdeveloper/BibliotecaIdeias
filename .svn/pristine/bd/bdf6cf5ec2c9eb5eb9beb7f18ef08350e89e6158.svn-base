body{
	margin: 0px;
	padding: 0;
	font-size: 12px;
	color: #616161;
	font-family: TAHOMA;
	font-color: black;
	background-color: #f8f8f8;
	width: 100%;
	height: 100%;
	}
	

.home-container{
	position: relative;
width: 70%;
height: auto;
float: left;
margin: 10% 15%;
}


#topo{
	
	background-image: url(../imagens/logosmall.jpg);
	float:left;
	height: 170px;
	width: 186px;
	color:red;
	background-repeat: no-repeat;
	
}

#topo{
	
	margin-left: 200px;
	margin-top: 240px;
	position: relative;
	
	
}

/* LOGIN AREA */

.home-container .login {
	float: left;
height: auto;
margin-top: 0px;
width: 100%;
position: relative;
}
.home-container .login-container {
	float: left;
height: auto;
margin-top: 0px;
width: 100%;
position: relative;
}

.home-container .loginarea
{
width: 40% !important;
height: auto !important;
border: 0px !important;
margin: 0 auto;
background: transparent;
border-radius: 0px !important;
position: relative;
}
.home-container .img-logo 
{
width: 100%;
margin: 0 auto;
float: left;
background: #fff;
border-radius: 0px !important;
box-shadow: #e8e8e8 0px 0px 4px 0px;
border-top: 1px solid #eeeeee;
border-left: 1px solid #eeeeee;
border-right: 1px solid #eeeeee;
border-bottom: 0px;
position: relative;
}
.home-container .img-logo img
{
width: 40%;
margin: 20px 30%;
position: relative;
}
.login-form{
	width: 100%;
clear: both;
float: right;
background-color: #313d65;
position: relative;
margin-right: -10%;
padding: 10%;
}
.login-form img{
float: left;
background-color: #fff;
position: relative;
padding: 2px 2px 2px 2px;
}
.login-form .login-input{
float: left;
background-color: #FFFFFF !important;
position: relative;
border: 0 !important;
border-radius: 0;
padding: 7px 6px;
width: 88%;
margin-bottom: 2%;
}
.logar-bt{
	width: 100%;
	border: 0 !important;
	border-radius: 0 !important;
	text-shadow: 0 !important;
	float: left;
	position: relative;
}
.msg-error{
	background: #fff !important;
	border-radius: 0 !important;
	float: left;
	position: relative;
	margin: 0 !important;
}
.msg-error .ui-messages-error{
	background: #fff !important;
	border-radius: 0 !important;
	border: 0 !important;
	color: #ff0000;
	float: left;
	position: relative;
	margin: 1px 0 !important;
	font-size: 10px !important;
	box-shadow: #e8e8e8 0px 0px 4px 0px;
	border-bottom: 1px solid #eeeeee;
	border-left: 1px solid #eeeeee;
	border-right: 1px solid #eeeeee;
	border-top: 0px;
}
.msg-error .ui-messages-error .ui-messages-error-icon{
	float: left;
	position: relative;
	margin-top: 0px;
}
.msg-error .ui-messages-error ul span{
	float: left;
	position: relative;
	margin-left: 10px !important;
	width: 102%;
}
