							header="Exclusão de Cliente" severity="alert"
							widgetVar="confirmationdelete" >

							<p:commandButton id="confirma" value="Sim"
							oncomplete="PF('confirmationdelete').hide()"
								action="#{editCliBean.excluirCliente()}"/>
								
								
							<p:commandButton id="declinedelete" value="Não"
								onclick="PF('confirmationdelete').hide()" type="button" />

						</p:confirmDialog>



 </h:form>

</div>


